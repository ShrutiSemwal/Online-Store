{
  "name": "online-store",
  "version": "1.0.0",
  "description": "This is an Online store web-app.",
  "main": " ",
  "author": "ShrutiSemwal",
  "watch": {
    "build": {
      "patterns": [
        "public"
      ],
      "extensions": "js,css"
    }
  },
  "scripts": {
    "serve": "node server.js",
