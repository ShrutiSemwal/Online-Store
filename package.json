    "npm-watch": "^0.11.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.31",
    "nodemon": "^2.0.13",
    "webpack-cli": "^4.8.0"
  }
